<div class="page-header">
  <h1>
    Users

    <a href="/users/new" class="btn btn-lg btn-success">New user</a>

  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>

        <th>User ID</th>

        <th>Username</th>

        <th>Email</th>

        <th>Created at</th>

        <th>Updated at</th>

        <!-- <th>Actions</th> -->

      </tr>

      <% @users.each do |user| %>
      <tr>

        <td><%= user.id %></td>

        <td><%= user.username %></td>

        <td><%= user.email %></td>

        <td><%= user.created_at %></td>

        <td><%= user.updated_at %></td>
        <!-- <td>


        </td> -->
      </tr>
      <% end %>
    </table>
  </div>
</div>
